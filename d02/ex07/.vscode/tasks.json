{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "C/C++: gcc build active file",
			"command": "/usr/bin/gcc",
			"args": [
				"-Wall",
				"-Wextra",
				"-Werror",
				"-g",
				"${workspaceFolder}/main.c",
				"${workspaceFolder}/ft_print_combn.c",
				"-o",
				"ft_print_combn"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build",
			"detail": "compiler: /usr/bin/gcc"
		}
	]
}
